            if (shyHeader.className.indexOf("visible") === -1) {
                shyHeader.className += " visible";
            }
        }
        else {
            shyHeader.className = shyHeader.className.replace(" visible", "");
        }
    }
    window.addEventListener("scroll", scrollShyHeader);
</script>


